CREATE OR REPLACE VIEW iv_vehicule_details FOLDER = '/2 - integration'
    PRIMARY KEY ( 'id_vehicule_details' ) AS SELECT iv_ads_localisation_all.id_vehicule_details AS id_vehicule_details, iv_ads_localisation_all.yearofregistration AS year_of_registration, iv_ads_localisation_all.monthofregistration AS month_of_registration, iv_ads_localisation_all.kilometer AS kilometer, case WHEN (iv_ads_localisation_all.monthofregistration = 0) THEN 'N/A' WHEN (iv_ads_localisation_all.monthofregistration = 1) THEN 'Janvier' WHEN (iv_ads_localisation_all.monthofregistration = 2) THEN 'Février' WHEN (iv_ads_localisation_all.monthofregistration = 3) THEN 'Mars' WHEN (iv_ads_localisation_all.monthofregistration = 4) THEN 'Avril' WHEN (iv_ads_localisation_all.monthofregistration = 5) THEN 'Mai' WHEN (iv_ads_localisation_all.monthofregistration = 6) THEN 'Juin' WHEN (iv_ads_localisation_all.monthofregistration = 7) THEN 'Juillet' WHEN (iv_ads_localisation_all.monthofregistration = 8) THEN 'Aout' WHEN (iv_ads_localisation_all.monthofregistration = 9) THEN 'Septembre' WHEN (iv_ads_localisation_all.monthofregistration = 10) THEN 'Octobre' WHEN (iv_ads_localisation_all.monthofregistration = 11) THEN 'Novembre' WHEN (iv_ads_localisation_all.monthofregistration = 12) THEN 'Décembre' END AS registration_month_alpha, case WHEN ((iv_ads_localisation_all.notrepaireddamage = '' OR iv_ads_localisation_all.notrepaireddamage is null )) THEN 'no damage' ELSE iv_ads_localisation_all.notrepaireddamage END AS not_repaired_damage FROM iv_ads_localisation_all;

ALTER VIEW iv_vehicule_details
 LAYOUT (iv_ads_localisation_all = [369, 138, 200, 200]);

